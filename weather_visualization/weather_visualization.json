{
  "ActionList": {
    "GetData": {
      "Arguments": {
        "folder_name": "WeatherVisualization",
        "output_name": "WeatherData.csv",
        "station_id": "USC00351862"
      },
      "InvokeNext": [
        "ProcessPrecipitation",
        "ProcessTemperatureMin",
        "ProcessTemperatureMax"
      ],
      "FaaSServer": "GH",
      "Type": "Python",
      "FunctionName": "get_ghcnd_data"
    },
    "ProcessPrecipitation": {
      "Arguments": {
        "folder_name": "WeatherVisualization",
        "input_name": "WeatherData.csv",
        "output_name": "PrecipitationData.csv",
        "column_name": "PRCP",
        "start": "2025-01-01",
        "end": "2025-03-01"
      },
      "InvokeNext": ["PlotData"],
      "FaaSServer": "GH",
      "Type": "Python",
      "FunctionName": "compare_to_yearly_average"
    },
    "ProcessTemperatureMin": {
      "FunctionName": "compare_to_yearly_average",
      "FaaSServer": "GH",
      "Arguments": {
        "folder_name": "WeatherVisualization",
        "input_name": "WeatherData.csv",
        "output_name": "TemperatureMinData.csv",
        "column_name": "TMIN",
        "start": "2025-01-01",
        "end": "2025-03-01"
      },
      "InvokeNext": ["PlotData"],
      "Type": "Python"
    },
    "ProcessTemperatureMax": {
      "FunctionName": "compare_to_yearly_average",
      "FaaSServer": "GH",
      "Arguments": {
        "folder_name": "WeatherVisualization",
        "input_name": "WeatherData.csv",
        "output_name": "TemperatureMaxData.csv",
        "column_name": "TMAX",
        "start": "2025-01-01",
        "end": "2025-03-01"
      },
      "InvokeNext": ["PlotData"],
      "Type": "Python"
    },
    "PlotData": {
      "Arguments": {
        "folder_name": "WeatherVisualization",
        "input_precip_name": "PrecipitationData.csv",
        "input_min_temp_name": "TemperatureMinData.csv",
        "input_max_temp_name": "TemperatureMaxData.csv",
        "location": "Corvallis,OR",
        "output_name": "WeatherComparison.png"
      },
      "InvokeNext": [],
      "FaaSServer": "GH",
      "Type": "Python",
      "FunctionName": "plot_weather_comparison"
    }
  },
  "ComputeServers": {
    "GH": {
      "FaaSType": "GitHubActions",
      "UseSecretStore": true,
      "ActionRepoName": "FaaSr-workflow",
      "Branch": "main",
      "UserName": "YOUR_USERNAME"
    }
  },
  "DataStores": {
    "S3": {
      "Endpoint": "https://play.min.io",
      "Bucket": "faasr",
      "Region": "us-east-1"
    }
  },
  "ActionContainers": {
    "GetData": "ghcr.io/faasr/github-actions-python:dev",
    "ProcessPrecipitation": "ghcr.io/faasr/github-actions-python:dev",
    "ProcessTemperatureMin": "ghcr.io/faasr/github-actions-python:dev",
    "PlotData": "ghcr.io/faasr/github-actions-python:dev",
    "ProcessTemperatureMax": "ghcr.io/faasr/github-actions-python:dev"
  },
  "FunctionInvoke": "GetData",
  "DefaultDataStore": "S3",
  "FunctionGitRepo": {
    "get_ghcnd_data": "JStover95/FaaSr-Functions/weather_visualization/python",
    "compare_to_yearly_average": "JStover95/FaaSr-Functions/weather_visualization/python",
    "plot_weather_comparison": "JStover95/FaaSr-Functions/weather_visualization/python"
  },
  "PyPIPackageDownloads": {
    "compare_to_yearly_average": ["pandas"],
    "plot_weather_comparison": ["pandas", "matplotlib"]
  },
  "FaaSrLog": "FaaSrLog",
  "LoggingDataStore": "S3",
  "WorkflowName": "WeatherVisualization"
}
