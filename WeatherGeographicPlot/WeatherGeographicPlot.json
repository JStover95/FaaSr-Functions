    }
  },
  "ActionContainers": {
    "GetData": "ghcr.io/faasr/github-actions-python:latest",
    "ProcessData": "ghcr.io/faasr/github-actions-python:latest",
    "PlotData": "ghcr.io/faasr/github-actions-python:latest"
  },
  "FunctionInvoke": "GetData",
