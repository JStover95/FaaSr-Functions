      "Arguments": {
        "folder_name": "WeatherGeographicPlot"
      },
      "InvokeNext": ["PlotData"],
      "Type": "Python"
    },
    "PlotData": {
      "FunctionName": "plot_county_weekly_temperature",
      "FaaSServer": "GH",
      "Arguments": {
        "folder_name": "WeatherGeographicPlot",
        "county_name": "Benton"
      },
      "InvokeNext": [],
      "Type": "Python"
    }
  },
  "ComputeServers": {
    "GH": {
      "FaaSType": "GitHubActions",
