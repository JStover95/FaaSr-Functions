  "FunctionGitRepo": {
    "get_geo_data_and_stations": "YOUR_USERNAME/FaaSr-Functions/WeatherGeographicPlot/python",
    "process_ghcnd_data": "YOUR_USERNAME/FaaSr-Functions/WeatherGeographicPlot/python",
